/* 백준 no.2920 음계 */

#include <iostream>
#define NOTE 8
using namespace std;

int main() {
	int arr[NOTE];

	// 연주 순서 입력받음
	for (int i = 0; i < NOTE; i++) {
		cin >> arr[i];
	}

	// 숫자 판별
	if (arr[0] == 1 && arr[1] == 2 && arr[2] == 3 && arr[3] == 4 && arr[4] == 5 && arr[5] == 6 && arr[6] == 7 && arr[7] == 8)
		cout << "ascending" << endl;
	else if (arr[0] == 8 && arr[1] == 7 && arr[2] == 6 && arr[3] == 5 && arr[4] == 4 && arr[5] == 3 && arr[6] == 2 && arr[7] == 1)
		cout << "descending" << endl;
	else
		cout << "mixed" << endl;

	return 0;
}
